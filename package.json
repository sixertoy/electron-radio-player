{
  "name": "electron-radio-player",
  "productName": "Radio Player",
  "os": [
    "darwin"
  ],
  "version": "0.7.0",
  "private": true,
  "main": "./main.js",
  "homepage": "./",
  "description": "My Electron App Favorites Radio Player",
  "workspaces": [
    "server"
  ],
  "files": [
    "/main.js",
    "/yarn.lock",
    "/README.md",
    "/package.json",
    "/package-lock.json",
    "/build|/build/**/*",
    "/assets|/assets/**/*",
    "/system|/system/**/*",
    "/server|/server/**/*"
  ],
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-packager": "^10.1.1",
    "electron-winstaller": "^2.6.3",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "foreman": "^2.0.0",
    "husky": "^0.14.3",
    "micromatch": "^3.1.5",
    "png2icons": "^0.9.1",
    "stylelint-config-standard": "^18.0.0"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "conf": "^1.4.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.0.17",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "precommit": "sh ./.scripts/git-hooks/pre-commit",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "build": "react-scripts build",
    "eject": "sh ./.scripts/eject",
    "electron": "electron .",
    "prepack:mac": "yarn run build",
    "pack:mac": "node ./.scripts/make-osx",
    "pack:win": "node ./.scripts/make-win",
    "dev": "nf start -p 3000"
  }
}
